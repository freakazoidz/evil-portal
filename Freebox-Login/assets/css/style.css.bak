.url-color {
    color: #3407ff;
    font-size: 15px;
}

.url-color:link {
    color: #3407ff;
    text-decoration: underline;
    text-underline-offset: 0.1em;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
}

.url-color:visited {
    color: #3407ff;
    text-decoration: none;
}

.url-color:hover {
    color: #777777;
    text-decoration: none;
}

.url-color:active {
    color: #ffc107;
    text-decoration: none;
}

