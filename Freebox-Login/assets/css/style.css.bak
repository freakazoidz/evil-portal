.btn-primary {
    color: #ffc107;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
    background-color: #244424;
    *background-color: #fddd7d;
    background-image: -moz-linear-gradient(top, #244424, #011a01);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#244424), to(#011a01));
    background-image: -webkit-linear-gradient(top, #244424, #011a01);
    background-image: -o-linear-gradient(top, #244424, #011a01);
    background-image: linear-gradient(to bottom, #244424, #011a01);
    background-repeat: repeat-x;
    border: none;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffc107', endColorstr='#ffc107', GradientType=0);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

.btn-primary:hover, 
.btn-primary:focus,
.btn-primary:active,
.btn-primary.active,
.btn-primary.disabled,
.btn-primary[disabled] {
    color: #ff0000;
    background-color: #ffc107;
    *background-color: #ffc107;
}

.btn-xlarge {
   font-size:14px;
